    "react-copy-to-clipboard": "^5.0.4",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-is": "^16.8.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
