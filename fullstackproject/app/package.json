    "@solana/wallet-adapter-react-ui": "^0.6.1",
    "@solana/wallet-adapter-wallets": "^0.11.3",
    "@solana/web3.js": "^1.31.0",
    "@terra-money/core": "^2.0.9",
    "@terra-money/terra.js": "^2.0.16",
    "@terra-money/use-wallet": "^3.3.0",
